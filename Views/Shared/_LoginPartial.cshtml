@using Microsoft.AspNetCore.Identity
@using AspNetCoreTodo.Models;
@using Microsoft.AspNetCore.Identity.EntityFrameworkCore
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> userManager

<ul class="navbar-nav">
@if (SignInManager.IsSignedIn(User))
{
    <li class="nav-item">
        <!--<a  class="nav-link text-dark" Acount/Manage/Index" title="Manage">Hello @User.Identity?.Name!</a>-->
        <div>
            <a class="nav-link text-dark" title="Manage">Hello @User.Identity?.Name!</a>
        </div>
        
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" href="/Account/Login">Logout</a>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link text-dark" href="/Account/Register">Register</a>

    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" href="/Account/Login">Login</a>
    </li>
}
</ul>
